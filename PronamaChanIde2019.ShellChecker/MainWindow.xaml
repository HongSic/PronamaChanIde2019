<Window
    x:Class="PronamaChanIde2019.ShellChecker.MainWindow"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:shells="clr-namespace:PronamaChanIde2019.Shells;assembly=PronamaChanIde2019.Shells"
    Title="MainWindow"
    Width="800"
    Height="450"
    mc:Ignorable="d">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition />
            <RowDefinition Height="Auto" />
            <RowDefinition Height="Auto" />
            <RowDefinition Height="Auto" />
            <RowDefinition Height="Auto" />
        </Grid.RowDefinitions>
        <shells:NineteenShell x:Name="MyShell" />
        <Button
            Grid.Row="1"
            Click="Success_Button_Click"
            Content="Sccess" />
        <Button
            Grid.Row="2"
            Click="Fail_Button_Click"
            Content="Fail" />
        <Button
            Grid.Row="3"
            Click="Blink_Button_Click"
            Content="Blink" />
        <Button
            Grid.Row="4"
            Click="ExpressEmotion_Button_Click"
            Content="Express Emotion" />
    </Grid>
</Window>
